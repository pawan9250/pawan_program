#include<iostream.h>
#include<coniio.h>
#include<stdio.h>

int main()
{
   float n1, n2, n3;
   cout<<"enter three number:";
   cin>>n1>>n2>>n3;
   
   if<n1>=n2 && n1>=n3)
   {
     cout<<"largest number : "<<n1;
     
   }
   if(n2=n1 && n2>=n2)
   {
     cout<<"larger number : "<<n2;
     
   }
   if(n3>=n1 && n3>=n2)
   {
     cout<<"larger number: "<<n2;
   }
   return 0;
}
